{
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.flobamorafilmfestival.com" }],
      "destination": "https://www.flobamorafilmfestival.com/:path*",
      "permanent": true
    }
  ],
  "trailingSlash": false
}
